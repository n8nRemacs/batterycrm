    {
      "parameters": {
        "method": "POST",
        "url": "http://217.145.79.27:8766/messages/text",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ]
        },
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={\n  \"sessionId\": \"{{ $('Get WhatsApp Session').item.json.session_id }}\",\n  \"to\": \"{{ $('Execute Workflow Trigger').item.json.out_message.external_chat_id }}\",\n  \"text\": \"{{ $('Execute Workflow Trigger').item.json.out_message.message_text.replace(/\"/g, '\\\\\"').replace(/\\n/g, '\\\\n') }}\"\n}",
        "options": {
          "timeout": 30000
        }
      },
      "id": "f519f27a-0a8e-49d7-919c-bbc380d8bc84",
      "name": "Send via Baileys",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [
        64,
        -128
      ]
    },
    {
      "parameters": {
        "jsCode": "const trigger = $('Execute Workflow Trigger').first().json;\nconst sessionData = $('Get WhatsApp Session').first().json;\nconst response = $input.first().json;\n\n// Check if send was successful\nconst success = response.success === true;\nconst messageId = response.data?.messageId || Date.now().toString();\n\n// Determine touchpoint direction\nconst actionSource = trigger.out_message?.action_source || '';\nconst touchpointDirection = ['promo', 'greeting', 'broadcast'].includes(actionSource) ? 'promo' : 'outbound';\n\nreturn {\n  ...trigger,\n  baileys_response: response,\n  message_id: messageId,\n  sent_at: new Date().toISOString(),\n  touchpoint_direction: touchpointDirection,\n  send_success: success,\n  session_id: sessionData.session_id\n};"
      },
      "id": "206b5b8a-8b22-4ce2-9e3a-680dbb8e9d0d",
      "name": "Process Baileys Response",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        288,
        -128
      ]
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "INSERT INTO elo_t_messages (\n  tenant_id, dialog_id, client_id, direction_id, actor_type, actor_id,\n  content, external_message_id, timestamp\n)\nVALUES (\n  '{{ $json.out_message.tenant_id }}'::uuid,\n  '{{ $json.out_message.dialog_id }}'::uuid,\n  '{{ $json.out_message.client_id }}'::uuid,\n  2,\n  '{{ $json.out_message.actor_type || \"operator\" }}',\n  {{ $json.out_message.actor_id ? \"'\" + $json.out_message.actor_id + \"'::uuid\" : \"NULL\" }},\n  '{{ $json.out_message.message_text.replace(/'/g, \"''\") }}',\n  '{{ $json.message_id }}',\n  NOW()\n)\nRETURNING id;",
        "options": {}
      },
      "id": "fe2a1703-940c-4938-aec9-028650ecd39f",
