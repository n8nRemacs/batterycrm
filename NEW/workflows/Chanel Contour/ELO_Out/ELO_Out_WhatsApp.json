{
  "name": "ELO_Out_WhatsApp",
  "active": true,
  "nodes": [
    {
      "parameters": {},
      "id": "51e468de-1b4f-4803-8b11-35ce03dfc607",
      "name": "Execute Workflow Trigger",
      "type": "n8n-nodes-base.executeWorkflowTrigger",
      "typeVersion": 1,
      "position": [-480, 112]
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "SELECT \n  ca.credentials->>'session_id' as session_id,\n  ca.account_id,\n  ca.account_name\nFROM elo_t_channel_accounts ca\nWHERE ca.tenant_id = '{{ $json.out_message.tenant_id }}'::uuid\n  AND ca.channel_id = 2\n  AND ca.is_active = true\nLIMIT 1;",
        "options": {}
      },
      "id": "get-whatsapp-session",
      "name": "Get WhatsApp Session",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.6,
      "position": [-256, 112],
      "credentials": {
        "postgres": {
          "id": "n2SyhP9QhMnp1ryk",
          "name": "Postgres account"
        }
      }
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict",
            "version": 1
          },
          "conditions": [
            {
              "id": "has-session",
              "leftValue": "={{ $json.session_id }}",
              "rightValue": "",
              "operator": {
                "type": "string",
                "operation": "exists",
                "singleValue": true
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "id": "if-session-exists",
      "name": "IF Session Exists",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [-32, 112]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "http://217.145.79.27:8766/messages/text",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ]
        },
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={\n  \"sessionId\": \"{{ $('Get WhatsApp Session').item.json.session_id }}\",\n  \"to\": \"{{ $('Execute Workflow Trigger').item.json.out_message.external_chat_id }}\",\n  \"text\": \"{{ $('Execute Workflow Trigger').item.json.out_message.message_text.replace(/\"/g, '\\\\\"').replace(/\\n/g, '\\\\n') }}\"\n}",
        "options": {
          "timeout": 30000
        }
      },
      "id": "send-whatsapp-baileys",
      "name": "Send via Baileys",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [192, 16]
    },
    {
      "parameters": {
        "jsCode": "const trigger = $('Execute Workflow Trigger').first().json;\nconst sessionData = $('Get WhatsApp Session').first().json;\nconst response = $input.first().json;\n\n// Check if send was successful\nconst success = response.success === true;\nconst messageId = response.data?.messageId || Date.now().toString();\n\n// Determine touchpoint direction\nconst actionSource = trigger.out_message?.action_source || '';\nconst touchpointDirection = ['promo', 'greeting', 'broadcast'].includes(actionSource) ? 'promo' : 'outbound';\n\nreturn {\n  ...trigger,\n  baileys_response: response,\n  message_id: messageId,\n  sent_at: new Date().toISOString(),\n  touchpoint_direction: touchpointDirection,\n  send_success: success,\n  session_id: sessionData.session_id\n};"
      },
      "id": "process-baileys-response",
      "name": "Process Baileys Response",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [416, 16]
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "INSERT INTO elo_t_messages (\n  tenant_id, dialog_id, client_id, direction_id, actor_type, actor_id,\n  content, external_message_id, timestamp\n)\nVALUES (\n  '{{ $json.out_message.tenant_id }}'::uuid,\n  '{{ $json.out_message.dialog_id }}'::uuid,\n  '{{ $json.out_message.client_id }}'::uuid,\n  2,\n  '{{ $json.out_message.actor_type || \"operator\" }}',\n  {{ $json.out_message.actor_id ? \"'\" + $json.out_message.actor_id + \"'::uuid\" : \"NULL\" }},\n  '{{ $json.out_message.message_text.replace(/'/g, \"''\") }}',\n  '{{ $json.message_id }}',\n  NOW()\n)\nRETURNING id;",
        "options": {}
      },
      "id": "save-outbound-message",
      "name": "Save Outbound Message",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.6,
      "position": [640, 16],
      "credentials": {
        "postgres": {
          "id": "n2SyhP9QhMnp1ryk",
          "name": "Postgres account"
        }
      }
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "UPDATE elo_t_dialogs SET\n  last_message_at = NOW(),\n  last_operator_message_at = NOW(),\n  updated_at = NOW()\nWHERE id = '{{ $('Execute Workflow Trigger').item.json.out_message.dialog_id }}'::uuid;",
        "options": {}
      },
      "id": "update-dialog-timestamp",
      "name": "Update Dialog Timestamp",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.6,
      "position": [864, 16],
      "credentials": {
        "postgres": {
          "id": "n2SyhP9QhMnp1ryk",
          "name": "Postgres account"
        }
      }
    },
    {
      "parameters": {
        "jsCode": "// Return error - no WhatsApp session configured\nconst trigger = $('Execute Workflow Trigger').first().json;\n\nreturn {\n  success: false,\n  error: 'No active WhatsApp session found for tenant',\n  tenant_id: trigger.out_message?.tenant_id,\n  out_message: trigger.out_message\n};"
      },
      "id": "no-session-error",
      "name": "No Session Error",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [192, 208]
    }
  ],
