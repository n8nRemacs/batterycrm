        "operation": "executeQuery",
        "query": "UPDATE appeal_devices\nSET\n  brand_id = CASE WHEN '{{ $json.brand_id }}' = 'undefined' THEN brand_id ELSE NULLIF('{{ $json.brand_id }}', '')::uuid END,\n  model_id = CASE WHEN '{{ $json.model_id }}' = 'undefined' THEN model_id ELSE NULLIF('{{ $json.model_id }}', '')::uuid END,\n  phone_model = CASE WHEN '{{ $json.phone_model }}' = 'undefined' THEN phone_model ELSE NULLIF('{{ $json.phone_model }}', '') END,\n  updated_at = NOW()\nWHERE id = '{{ $json.device_id }}'::uuid\n  AND tenant_id = '{{ $json.tenant_id }}'::uuid\nRETURNING id, brand_id, model_id, phone_model;",
        "options": {}
