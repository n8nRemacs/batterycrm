    {
      "parameters": {
        "operation": "executeQuery",
        "query": "INSERT INTO appeal_devices (appeal_id, tenant_id, brand_id, model_id, phone_model, device_order)\nSELECT\n  '{{ $json.appeal_id }}'::uuid,\n  '{{ $json.tenant_id }}'::uuid,\n  NULLIF('{{ $json.brand_id }}', '')::uuid,\n  NULLIF('{{ $json.model_id }}', '')::uuid,\n  NULLIF('{{ $json.phone_model }}', ''),\n  COALESCE((SELECT MAX(device_order) + 1 FROM appeal_devices WHERE appeal_id = '{{ $json.appeal_id }}'::uuid), 1)\nWHERE EXISTS (\n  SELECT 1 FROM appeals WHERE id = '{{ $json.appeal_id }}'::uuid AND tenant_id = '{{ $json.tenant_id }}'::uuid\n)\nRETURNING id, device_order, brand_id, model_id, phone_model;",
        "options": {}
