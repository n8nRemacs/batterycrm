            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "id": "token-exists",
              "leftValue": "={{ $json.session_token }}",
              "rightValue": "",
              "operator": {
                "type": "string",
                "operation": "notEmpty",
                "singleValue": true
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "id": "a4cc0357-a566-49d3-a837-a0c79d585f87",
      "name": "IF - Token Provided",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
