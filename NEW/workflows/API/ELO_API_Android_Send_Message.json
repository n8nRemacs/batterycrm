    {
      "parameters": {
        "operation": "executeQuery",
        "query": "INSERT INTO elo_t_messages (\n  tenant_id,\n  dialog_id,\n  client_id,\n  direction_id,\n  message_type_id,\n  actor_type,\n  actor_id,\n  content,\n  media,\n  timestamp\n) VALUES (\n  '{{ $('Postgres - Get Dialog Info').item.json.tenant_id }}'::uuid,\n  '{{ $('Postgres - Get Dialog Info').item.json.dialog_id }}'::uuid,\n  '{{ $('Postgres - Get Dialog Info').item.json.client_id }}'::uuid,\n  2,\n  {{ $('Webhook - Send Message').item.json.body.media_type === 'voice' ? 2 : 1 }},\n  'operator',\n  '{{ $('Postgres - Validate Session').item.json.operator_id }}'::uuid,\n  '{{ $('Webhook - Send Message').item.json.body.text.replace(/'/g, \"''\") }}',\n  '{{ $('Webhook - Send Message').item.json.body.media_data ? JSON.stringify({type: $('Webhook - Send Message').item.json.body.media_type, data: $('Webhook - Send Message').item.json.body.media_data}) : '{}' }}'::jsonb,\n  NOW()\n)\nRETURNING id, content as text, 'out' as direction, 'operator' as sender_type, \n  '{{ $('Postgres - Validate Session').item.json.operator_name }}' as sender_name,\n  EXTRACT(EPOCH FROM timestamp)::bigint * 1000 as timestamp;",
        "options": {}
      },
      "id": "e3f786d0-e2f9-496d-9501-823a06cb9a29",
      "name": "Postgres - Insert Message",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.6,
