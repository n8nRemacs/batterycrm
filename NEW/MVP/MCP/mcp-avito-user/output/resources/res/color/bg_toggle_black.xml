<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item
        android:state_enabled="false"
        android:state_checked="false"
        android:color="@color/look_and_feel_warm_gray_8"/>
    <item
        android:state_enabled="false"
        android:state_checked="true"
        android:color="@color/look_and_feel_warm_gray_28"/>
    <item
        android:state_checked="false"
        android:state_pressed="true"
        android:color="@color/look_and_feel_warm_gray_12"/>
    <item
        android:state_checked="true"
        android:color="@color/look_and_feel_black"/>
    <item
        android:color="@color/look_and_feel_black"
        app:state_indeterminate="true"/>
    <item
        android:state_checked="false"
        android:color="@color/look_and_feel_warm_gray_8"/>
</selector>
