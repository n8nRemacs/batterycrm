<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:gravity="left"
        android:height="7dp"
        android:width="7dp"
        android:start="@dimen/day_schedule_timeline_start">
        <shape android:shape="oval">
            <solid android:color="?attr/black"/>
        </shape>
    </item>
    <item
        android:start="@dimen/day_schedule_timeline_start"
        android:end="@dimen/day_schedule_timeline_end">
        <shape android:shape="line">
            <stroke
                android:width="1dp"
                android:color="?attr/black"/>
        </shape>
    </item>
</layer-list>
