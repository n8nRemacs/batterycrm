<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <ripple android:color="@color/legacy_black_alpha_32">
            <item android:drawable="@drawable/design_system_primary_floating_buttons_normal"/>
        </ripple>
    </item>
    <item
        android:state_focused="true"
        android:state_enabled="false"
        android:drawable="@drawable/design_system_primary_floating_buttons_disabled_focused"/>
    <item
        android:state_enabled="false"
        android:drawable="@drawable/design_system_primary_floating_buttons_disabled"/>
</selector>
