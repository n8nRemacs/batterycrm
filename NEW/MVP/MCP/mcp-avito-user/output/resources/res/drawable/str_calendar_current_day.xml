<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <ripple android:color="@color/legacy_black_alpha_32">
            <item android:id="@android:id/mask">
                <shape android:shape="rectangle">
                    <corners android:radius="12dp"/>
                    <solid android:color="?attr/warmGray4"/>
                </shape>
            </item>
        </ripple>
    </item>
    <item>
        <shape android:shape="rectangle">
            <size
                android:height="@dimen/calendar_recycler_view_day_size"
                android:width="@dimen/calendar_recycler_view_day_size"/>
            <stroke
                android:width="1dp"
                android:color="?attr/black"/>
            <corners android:radius="12dp"/>
        </shape>
    </item>
</selector>
