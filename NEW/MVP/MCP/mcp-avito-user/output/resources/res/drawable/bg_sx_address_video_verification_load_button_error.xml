<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <ripple android:color="?attr/warmGray8">
            <item>
                <shape>
                    <solid android:color="?attr/warmGray4"/>
                    <corners android:radius="16dp"/>
                </shape>
            </item>
            <item android:id="@android:id/mask">
                <shape>
                    <solid android:color="?attr/white"/>
                    <corners android:radius="15dp"/>
                </shape>
            </item>
        </ripple>
    </item>
    <item>
        <shape>
            <solid android:color="?attr/transparentBlack"/>
            <stroke
                android:width="1dp"
                android:color="?attr/red600"/>
            <corners android:radius="16dp"/>
        </shape>
    </item>
</layer-list>
