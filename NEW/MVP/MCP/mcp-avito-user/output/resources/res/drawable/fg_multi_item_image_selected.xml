<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/multi_item_image_corner_radius"/>
            <stroke
                android:width="2dp"
                android:color="?attr/black"/>
        </shape>
    </item>
    <item
        android:left="2dp"
        android:top="2dp"
        android:right="2dp"
        android:bottom="2dp">
        <shape android:shape="rectangle">
            <corners android:radius="10dp"/>
            <stroke
                android:width="2dp"
                android:color="?attr/white"/>
        </shape>
    </item>
</layer-list>
