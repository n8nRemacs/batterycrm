<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:paddingTop="12dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <include
        android:id="@+id/select_agent_content"
        android:visibility="visible"
        layout="@layout/select_agent_dialog_content"/>
    <include
        android:id="@+id/select_agent_placeholders"
        android:visibility="gone"
        layout="@layout/select_agent_dialog_placeholders"/>
    <include
        android:id="@+id/select_agent_error"
        android:visibility="gone"
        layout="@layout/common_error"/>
    <include
        android:id="@+id/select_agent_empty"
        android:visibility="gone"
        layout="@layout/select_agent_dialog_empty"/>
</FrameLayout>
