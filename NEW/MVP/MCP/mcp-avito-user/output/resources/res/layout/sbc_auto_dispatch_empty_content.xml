<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:paddingLeft="16dp"
    android:paddingRight="16dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:textAppearance="?attr/textH30"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/sbc_auto_dispatch_empty_content_title"/>
    <TextView
        android:textAppearance="?attr/textM10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="@string/sbc_auto_dispatch_empty_content_body"/>
    <FrameLayout
        android:id="@+id/sbc_auto_dispatch_empty_accordion_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"/>
</LinearLayout>
