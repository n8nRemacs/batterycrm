<?xml version="1.0" encoding="utf-8"?>
<com.avito.android.lib.design.floating_container.FloatingContainer xmlns:android="http://schemas.android.com/apk/res/android"
    android:theme="@style/Theme.DesignSystem.Re23"
    android:layout_gravity="bottom|center_horizontal"
    android:orientation="vertical"
    android:id="@+id/buttons_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="16dp"
    style="?attr/floatingContainerIncomplete">
    <com.avito.android.lib.design.button.Button
        android:layout_gravity="bottom|center_horizontal"
        android:id="@+id/main_button"
        android:layout_width="match_parent"
        style="?attr/buttonPrimaryLarge"/>
</com.avito.android.lib.design.floating_container.FloatingContainer>
