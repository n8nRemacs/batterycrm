<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:id="@+id/bbip_budget_container"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:textAppearance="?attr/textH5"
        android:id="@+id/bbip_budget_title_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/bbip_content_margin_horizontal"/>
    <androidx.compose.ui.platform.ComposeView
        android:id="@+id/bbip_budget_compose_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</LinearLayout>
