<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:theme="@style/Theme.DesignSystem.Re23"
    android:id="@+id/imv_services_dialog_good_chart_root"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:textAppearance="?attr/textH6"
        android:textColor="?attr/green800"
        android:id="@+id/imv_services_good_from"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        app:layout_constraintStart_toEndOf="@+id/imv_services_good_border_start"
        app:layout_constraintTop_toTopOf="parent"/>
    <TextView
        android:textAppearance="?attr/textH6"
        android:textColor="?attr/green800"
        android:id="@+id/imv_services_good_to"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toStartOf="@+id/imv_services_good_border_end"
        app:layout_constraintTop_toTopOf="parent"/>
    <View
        android:id="@+id/imv_services_good_border_start"
        android:background="@drawable/bg_imv_services_dialog_track"
        android:layout_width="16dp"
        android:layout_height="4dp"
        android:backgroundTint="?attr/gray28"
        app:layout_constraintBottom_toBottomOf="@+id/imv_services_good_from"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imv_services_good_from"
        app:layout_constraintVertical_bias="0.6"/>
    <View
        android:id="@+id/imv_services_good_border_end"
        android:background="@drawable/bg_imv_services_dialog_track"
        android:layout_width="16dp"
        android:layout_height="4dp"
        android:backgroundTint="?attr/gray28"
        app:layout_constraintBottom_toBottomOf="@+id/imv_services_good_to"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imv_services_good_to"
        app:layout_constraintVertical_bias="0.6"/>
    <View
        android:id="@+id/imv_services_good_track_center"
        android:background="?attr/green600"
        android:layout_width="0dp"
        android:layout_height="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/imv_services_good_track_center_start_part"
        app:layout_constraintEnd_toStartOf="@+id/imv_services_good_track_center_end_part"
        app:layout_constraintStart_toEndOf="@+id/imv_services_good_track_center_start_part"
        app:layout_constraintTop_toTopOf="@+id/imv_services_good_track_center_start_part"/>
    <View
        android:id="@+id/imv_services_good_track_center_start_part"
        android:background="@drawable/bg_imv_services_dialog_start_track_shadow"
        android:layout_width="8dp"
        android:layout_height="4dp"
        android:layout_marginStart="2dp"
        android:backgroundTint="?attr/green600"
        app:layout_constraintBottom_toBottomOf="@+id/imv_services_good_from"
        app:layout_constraintStart_toEndOf="@+id/imv_services_good_from"
        app:layout_constraintTop_toTopOf="@+id/imv_services_good_from"/>
    <View
        android:id="@+id/imv_services_good_track_center_end_part"
        android:background="@drawable/bg_imv_services_dialog_end_track_shadow"
        android:layout_width="8dp"
        android:layout_height="4dp"
        android:layout_marginEnd="2dp"
        android:backgroundTint="?attr/green600"
        app:layout_constraintBottom_toBottomOf="@+id/imv_services_good_to"
        app:layout_constraintEnd_toStartOf="@+id/imv_services_good_to"
        app:layout_constraintTop_toTopOf="@+id/imv_services_good_to"/>
    <ImageView
        android:id="@+id/imv_services_good_track_point"
        android:background="@drawable/bg_imv_services_dialog_track_point"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:foreground="@drawable/fg_imv_services_dialog_track_point"
        android:foregroundGravity="center"
        android:foregroundTint="?attr/green800"
        app:layout_constraintBottom_toBottomOf="@+id/imv_services_good_track_center"
        app:layout_constraintEnd_toEndOf="@+id/imv_services_good_track_center"
        app:layout_constraintStart_toStartOf="@+id/imv_services_good_track_center"
        app:layout_constraintTop_toTopOf="@+id/imv_services_good_track_center"/>
    <TextView
        android:textAppearance="?attr/textXs"
        android:id="@+id/imv_services_good_track_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imv_services_good_track_center_end_part"
        app:layout_constraintStart_toStartOf="@+id/imv_services_good_track_center_start_part"
        app:layout_constraintTop_toBottomOf="@+id/imv_services_good_track_center"/>
</androidx.constraintlayout.widget.ConstraintLayout>
