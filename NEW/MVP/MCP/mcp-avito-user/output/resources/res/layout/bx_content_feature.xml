<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/bx_content_feature_root"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include layout="@layout/bx_content_bottom_sheet"/>
    <include
        android:visibility="gone"
        layout="@layout/shortcuts"/>
    <include
        android:visibility="gone"
        layout="@layout/global_shortcuts"/>
    <include
        android:layout_gravity="bottom|center_horizontal"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        layout="@layout/recs_reload_button"/>
    <include
        android:layout_gravity="bottom|center_horizontal"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/bottom_entry_point_tabbar_margin"
        layout="@layout/filters_bottom_entry_point"/>
    <include
        android:layout_gravity="start|bottom"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/inline_filters_map_bottom_entry_point_bottom_margin"
        android:layout_marginStart="12dp"
        layout="@layout/inline_filters_map_bottom_entry_point"/>
    <include
        android:layout_gravity="bottom"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/floating_promo_widget_item"/>
    <include
        android:layout_gravity="bottom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/search_map_bottom_container"/>
    <ViewStub
        android:id="@+id/search_map_progress_animated_overlay_container_stub"
        android:visibility="gone"
        android:layout="@layout/search_map_animated_overlay"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <ViewStub
        android:id="@+id/search_map_progress_default_overlay_container_stub"
        android:visibility="gone"
        android:layout="@layout/search_map_default_overlay"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <FrameLayout
        android:id="@+id/replace_main_onboarding_container"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
</androidx.coordinatorlayout.widget.CoordinatorLayout>
