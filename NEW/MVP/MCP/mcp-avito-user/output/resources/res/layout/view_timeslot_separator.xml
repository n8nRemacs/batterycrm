<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:textAppearance="?attr/textBodySmall"
        android:textColor="?attr/gray54"
        android:id="@+id/timeslot_separator_title"
        android:layout_width="@dimen/day_schedule_timeslot_section_separator_time_width"
        android:layout_height="wrap_content"
        android:paddingStart="16dp"
        android:paddingEnd="0dp"/>
    <View
        android:background="@drawable/timeslot_separator"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:layout_marginEnd="10dp"/>
</merge>
