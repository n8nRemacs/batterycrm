<?xml version="1.0" encoding="utf-8"?>
<com.avito.android.lib.design.skeleton.shimmer.ShimmerLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:shimmer="http://schemas.android.com/apk/res-auto"
    android:id="@+id/shortcuts_header_skeleton"
    android:paddingTop="6dp"
    android:paddingBottom="6dp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingStart="@dimen/serp_horizontal_padding"
    shimmer:shimmer_animationDuration="3000"
    shimmer:shimmer_autostart="true"
    shimmer:shimmer_color="@color/legacy_gray_shimmer"
    shimmer:shimmer_gradientCenterColorWidth="0.1"
    shimmer:shimmer_maskWidth="1">
    <LinearLayout
        android:orientation="horizontal"
        android:clipToPadding="false"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingStart="4dp">
        <View
            android:background="@drawable/serp_navigation_button_skeleton_stub"
            android:layout_width="196dp"
            android:layout_height="16dp"
            android:layout_marginRight="10dp"/>
        <View
            android:background="@drawable/serp_navigation_button_skeleton_stub"
            android:layout_width="196dp"
            android:layout_height="16dp"/>
    </LinearLayout>
</com.avito.android.lib.design.skeleton.shimmer.ShimmerLayout>
