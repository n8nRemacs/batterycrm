<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/map_container"
    android:layout_width="match_parent"
    android:layout_height="@dimen/map_default_height"
    app:cardCornerRadius="@dimen/map_default_corner_radius"
    app:cardElevation="0dp"
    app:cardUseCompatPadding="false"
    style="@style/AvitoCardView">
    <com.google.android.gms.maps.MapView
        android:id="@+id/beduin_google_map"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:liteMode="true"
        app:mapType="none"/>
</androidx.cardview.widget.CardView>
