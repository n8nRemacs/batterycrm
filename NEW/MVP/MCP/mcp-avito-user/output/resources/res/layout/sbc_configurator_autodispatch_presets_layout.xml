<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:weightSum="1">
        <include
            android:id="@+id/sbc_configurator_auto_dispatch_common_preset_1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.5"
            android:layout_marginEnd="6dp"
            layout="@layout/sbc_configurator_autodispatch_common_preset"/>
        <include
            android:id="@+id/sbc_configurator_auto_dispatch_common_preset_2"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.5"
            android:layout_marginStart="6dp"
            layout="@layout/sbc_configurator_autodispatch_common_preset"/>
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="6dp"
        android:weightSum="1">
        <include
            android:id="@+id/sbc_configurator_auto_dispatch_common_preset_3"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.5"
            android:layout_marginEnd="6dp"
            layout="@layout/sbc_configurator_autodispatch_common_preset"/>
        <include
            android:id="@+id/sbc_configurator_auto_dispatch_another_preset"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.5"
            android:layout_marginStart="6dp"
            layout="@layout/sbc_configurator_autodispatch_another_preset"/>
    </LinearLayout>
</LinearLayout>
