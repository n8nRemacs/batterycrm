<?xml version="1.0" encoding="utf-8"?>
<com.avito.android.lib.design.component_container.ComponentContainer xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:theme="@style/Theme.DesignSystem.Re23"
    android:orientation="vertical"
    android:id="@+id/photo_param_container"
    android:paddingTop="3dp"
    android:paddingBottom="?attr/publish_componentContainer_paddingBottom"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingHorizontal="?attr/publish_horizontalPadding"
    app:componentContainer_contentPaddingBottom="@dimen/publish_container_content_bottom_padding"
    app:componentContainer_contentPaddingTop="16dp"
    app:componentContainer_titleAppearance="?attr/publish_select_titleTextAppearance"
    app:componentContainer_titleTipIcon="?attr/publish_componentContainer_titleTipIcon"
    app:componentContainer_titleTipIconTint="?attr/publish_componentContainer_titleTipIconTint">
    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/image_list_container"
        android:clipChildren="false"
        android:clipToPadding="false"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:nestedScrollingEnabled="false"
        style="?attr/photoUploaderList"/>
</com.avito.android.lib.design.component_container.ComponentContainer>
