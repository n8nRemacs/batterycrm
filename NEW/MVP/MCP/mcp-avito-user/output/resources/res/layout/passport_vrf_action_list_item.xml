<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/passport_vrf_action_list_item_root"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/passport_vrf_action_list_item_left_icon"
        android:layout_width="14dp"
        android:layout_height="20dp"
        android:layout_marginTop="11dp"
        android:layout_marginStart="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        style="?attr/textM20"/>
    <TextView
        android:textAppearance="?attr/textM20"
        android:id="@+id/passport_vrf_action_list_item_title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="12dp"
        app:layout_constraintEnd_toStartOf="@+id/passport_vrf_action_list_item_right_icon"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toEndOf="@+id/passport_vrf_action_list_item_left_icon"
        app:layout_constraintTop_toTopOf="@+id/passport_vrf_action_list_item_left_icon"/>
    <TextView
        android:textAppearance="?attr/textS20"
        android:textColor="?attr/gray54"
        android:id="@+id/passport_vrf_action_list_item_subtitle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_marginBottom="13dp"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/passport_vrf_action_list_item_right_icon"
        app:layout_constraintStart_toEndOf="@+id/passport_vrf_action_list_item_left_icon"
        app:layout_constraintTop_toBottomOf="@+id/passport_vrf_action_list_item_title"/>
    <TextView
        android:gravity="end|bottom"
        android:id="@+id/passport_vrf_action_list_item_right_icon"
        android:layout_width="8dp"
        android:layout_height="20dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="22dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        style="?attr/textM20"/>
</androidx.constraintlayout.widget.ConstraintLayout>
