# Tunnel Connection
TUNNEL_URL=wss://tunnel.eldoleado.ru/ws
TUNNEL_SECRET=your_secret_here
SERVER_ID=phone_1

# Tenant/Proxy Settings
TENANT_ID=your_tenant_id          # Required for proxy registration
NODE_TYPE=operator                 # "operator" or "client"
WIFI_ONLY=true                     # Only use proxy on WiFi
MAX_REQUESTS_PER_HOUR=10           # Rate limit for proxy requests
STATUS_UPDATE_INTERVAL=60          # How often to send status updates (seconds)

# Reconnection
RECONNECT_DELAY=5
MAX_RECONNECT_DELAY=60
HTTP_TIMEOUT=30

# Telegram (from my.telegram.org)
TELEGRAM_API_ID=your_api_id
TELEGRAM_API_HASH=your_api_hash
TELEGRAM_PHONE=+79001234567

# Avito
AVITO_SESSID=              # sessid cookie from browser
AVITO_ACCESS_TOKEN=        # OAuth access token
AVITO_REFRESH_TOKEN=       # OAuth refresh token

# MAX (VK Teams)
MAX_TOKEN=                 # Token from login
MAX_DEVICE_ID=             # Device ID (auto-generated if empty)

# VK
VK_ACCESS_TOKEN=

# Logging
LOG_LEVEL=INFO
